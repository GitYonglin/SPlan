              </view>
            </view>
            <view class="other">
              <view>
                <p class="price">￥9999.99</p>
                <span class="or">/</span>
                <p class="unit">9999.99g</p>
              </view>
              <view>
                <text class='inventory'>库99+</text>
              </view>
            </view>
          </view>
    </view>
  </view>

